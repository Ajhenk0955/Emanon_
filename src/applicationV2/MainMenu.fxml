<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="653.0" styleClass="background" stylesheets="@JMetroDarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="applicationV2.MainMenuController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="653.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <MenuBar id="menu pane" stylesheets="@JMetroDarkTheme.css" BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem fx:id="logOutItem" mnemonicParsing="false" onAction="#handleMenuBar" text="Log out " />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem fx:id="edit_UserProfile" mnemonicParsing="false" onAction="#handleUserProfile" text="Edit Profile..." />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
      </BorderPane>
      <Label layoutX="8.0" layoutY="33.0" text="Main Menu" textFill="#fcf6f6">
         <font>
            <Font size="49.0" />
         </font>
      </Label>
      <VBox layoutX="14.0" layoutY="123.0" prefHeight="118.0" prefWidth="161.0">
         <children>
          <Button fx:id="searchButton" mnemonicParsing="false" onAction="#handleSearchButton" prefHeight="33.0" prefWidth="164.0" text="Search Patient" />
            <Button fx:id="createPatientButton" mnemonicParsing="false" onAction="#handleAddPatientButton" prefHeight="33.0" prefWidth="163.0" text="Add New Patient" />
            <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#handleMMLogoutButton" text="Log out" />
         </children>
      </VBox>
      <TextField layoutX="327.0" layoutY="106.0" prefHeight="33.0" prefWidth="225.0" promptText="........" />
      <Label layoutX="352.0" layoutY="85.0" text="Quick Search" textFill="#fcf6f6">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="quickSearch" layoutX="552.0" layoutY="106.0" mnemonicParsing="false" onAction="#handleSearchButton" text="Go!" />
   </children>
</AnchorPane>
